<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="30.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerJoinGame">
    <children>
        <Label fx:id="label_title" text="MULTIPLAYER">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
      <ListView fx:id="listView_openGames" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
      <Button fx:id="button_joinGame" mnemonicParsing="false" onAction="#joinGame" text="join game" />
    </children>
    <padding>
        <Insets bottom="30.0" left="70.0" right="70.0" top="30.0" />
    </padding>
</VBox>
