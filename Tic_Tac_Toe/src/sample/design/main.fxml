<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" spacing="10.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerMain">
    <children>
        <Label fx:id="label_title" text="TIC TAC TOE">
            <font>
                <Font size="28.0" />
            </font>
            <padding>
                <Insets bottom="30.0" />
            </padding>
        </Label>
      <Button mnemonicParsing="false" text="watch game stream" />
        <Button fx:id="button_lan" mnemonicParsing="false" onAction="#multiplayer" text="multiplayer" />
        <Button fx:id="button_ai" mnemonicParsing="false" onAction="#switchAI" text="ai" />
      <Button fx:id="button_userProfile" mnemonicParsing="false" onAction="#loadProfile" text="user profile" />
      <Button fx:id="button_logOut" mnemonicParsing="false" onAction="#logOut" text="log out" />
    </children>
    <padding>
        <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
    </padding>
</VBox>
